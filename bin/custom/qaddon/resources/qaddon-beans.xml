<?xml version="1.0" encoding="ISO-8859-1"?>

<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="questions" type="java.util.List&lt;questions.data.QuestionData>"/>
	</bean>

	<bean class="questions.data.QuestionData">
		<property name="code" type="String"/>
		<property name="question" type="String"/>
		<property name="answer" type="String"/>
		<property name="questionCustomer" type="de.hybris.platform.commercefacades.user.data.PrincipalData"/>
		<property name="answerCustomer" type="de.hybris.platform.commercefacades.user.data.PrincipalData"/>
	</bean>
</beans>
